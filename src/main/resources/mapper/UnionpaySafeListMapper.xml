<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.microfin.logic.dao.UnionpaySafeListDao">
    <select id="query" resultType="com.microfin.logic.entity.UnionpaySafeList">
        SELECT
          province,city,orgName,busCategory,address,manageOrg,certifyDate
        FROM
          T_UNIONPAY_SAFE_LIST
        <where>
            <if test="_parameter !=null and _parameter != '' ">
                AND
                dataid = #{id,jdbcType=VARCHAR}
            </if>
        </where>
    </select>
</mapper>